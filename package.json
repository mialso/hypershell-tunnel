{
  "name": "hypershell-tunnel",
  "version": "1.0.0",
  "description": "tunnel example",
  "license": "ISC",
  "author": "mialso",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "init:keyfile:lm": "hypershell-keygen -f ./lm-keyfile",
    "init:keyfile:mx": "hypershell-keygen -f ./mx-keyfile",
    "init:keyfile:ssh": "hypershell-keygen -f ./ssh-keyfile",
    "start:server:lm": "hypershell-server -f ./lm-keyfile --firewall ./lm-firewall --protocol tunnel --tunnel-host 127.0.0.1 --tunnel-port 3082",
    "start:server:mx": "hypershell-server -f ./mx-keyfile --firewall ./mx-firewall --protocol tunnel --tunnel-host 127.0.0.1 --tunnel-port 3081",
    "start:server:ssh": "hypershell -f ./ssh-keyfile --firewall ./ssh-firewall",
    "start:client:lm": "hypershell -f ./lm-keyfile lm-provider -L 127.0.0.1:8082:127.0.0.1:3082",
    "start:client:mx": "hypershell -f ./mx-keyfile mx-provider -L 127.0.0.1:8081:127.0.0.1:3081",
    "start:client:ssh": "hypershell -f ./ssh-keyfile ssh-provider",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "hypershell": "0.0.15"
  }
}
